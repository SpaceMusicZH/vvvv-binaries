namespace VL.Stride.Rendering.Images
{
    /// <summary>
    /// A gaussian blur effect
    /// </summary>
    effect AmbientOcclusionWithOrthoBlurEffect
    {
        using params AmbientOcclusionWithOrthoBlurKeys;

        // Mixin 
        mixin AmbientOcclusionWithOrthoBlurShader<AmbientOcclusionWithOrthoBlurKeys.Count, AmbientOcclusionWithOrthoBlurKeys.VerticalBlur, AmbientOcclusionWithOrthoBlurKeys.BlurScale, AmbientOcclusionWithOrthoBlurKeys.EdgeSharpness, AmbientOcclusionWithOrthoBlurKeys.IsOrtho>;
    };
}
